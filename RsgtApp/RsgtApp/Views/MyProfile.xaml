						IsVisible="{Binding IsvalidatedEmail}"
						Margin="0,0,0,5" 
						FontFamily="Montserrat-Regular" 
						TextColor="Red" 
						FontSize="18"
						HorizontalOptions="StartAndExpand"/>
                        <Label  Text="{Binding MsgEmailValid}" 
						IsVisible="{Binding IsvalidatedEmailID}"
						Margin="0,0,0,5" 
						FontFamily="Montserrat-Regular"
						TextColor="Red" 
