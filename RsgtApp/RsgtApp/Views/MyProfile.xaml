                                HorizontalOptions="StartAndExpand"/>
                        <Label  Text="{Binding lblPrefComm1}" 
						Margin="0,10,0,0" 
						StyleClass="textsemibold" 
						FontSize="18" 
						TextColor="{DynamicResource Primary}" 
						HorizontalOptions="StartAndExpand"/>
                        <Picker AutomationId="APckPrefComm"
									ItemsSource="{Binding lobjPrefComm}"                                    
                                    ItemDisplayBinding="{Binding Key}"                                    
                                    SelectedItem="{Binding SelectedPrefComm}" 
									Title="{Binding picPrefComm}"
									FontFamily="Montserrat-Regular" 
									TitleColor="#b3b2b2" 
									StyleClass="Pickertextregular"
									TextColor="{DynamicResource Primary}"
                                    BackgroundColor="{DynamicResource Entrytextbg}"
									Margin="0,0,0,0"/>
                        <Label Text="{Binding MsgPrefComm}" 
						IsVisible="{Binding IsvalidatedPrefComm}"
						Margin="0,0,0,5"
						FontFamily="Montserrat-Regular"
						TextColor="Red" 
						FontSize="18"
						HorizontalOptions="StartAndExpand"/>
                        <Label  Text="{Binding lblJobTitle}"
						Margin="0,10,0,0" 
						StyleClass="textsemibold" 
						FontSize="18" 
						TextColor="{DynamicResource Primary}" 
						HorizontalOptions="StartAndExpand"/>
                        <Entry AutomationId="AEntJobTitlePlc"
                        Text="{Binding TxtJobTitle}" 
						Placeholder="{Binding PlaceJobTitle}"
						StyleClass="Entrytextregular" 
						TextColor="{DynamicResource Primary}"
                        BackgroundColor="{DynamicResource Entrytextbg}"
						Margin="0,0,0,0" />
                        <Label Text="{Binding MsgjobTitle}" 
						IsVisible="{Binding IsvalidatedJobtitle}"
						Margin="0,0,0,5"
						FontFamily="Montserrat-Regular"
						TextColor="Red" 
						FontSize="18"
						HorizontalOptions="StartAndExpand"/>
                        <Label  x:Name="lblJobFunction" 
						Text="{Binding lblJobFunction}" 
						Margin="0,10,0,0" 
						StyleClass="textsemibold" 
						FontSize="18" 
						TextColor="{DynamicResource Primary}" 
						HorizontalOptions="StartAndExpand"/>
                        <Picker AutomationId="ApckJobFunct" 
