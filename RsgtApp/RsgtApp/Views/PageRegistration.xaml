						Text="{Binding lblPostCode}"
						StyleClass="textsemibold" 
						TextColor="{DynamicResource Primary}" 
						FontSize="18" 
						HorizontalOptions="StartAndExpand"/>
                        <Label  Text="{Binding lblPostCodeVal}"
						StyleClass="textregular" 
						TextColor="{DynamicResource Primary}"
						FontSize="18" 
						HorizontalOptions="StartAndExpand"/>
                        <Label  Text="{Binding lblAdditionalNo}" 
						StyleClass="textsemibold" 
						TextColor="{DynamicResource Primary}"
						FontSize="18" 
						HorizontalOptions="StartAndExpand"/>
                        <Label Text="{Binding lblAdditionalNoVal}" 
						StyleClass="textregular"
						TextColor="{DynamicResource Primary}" 
						FontSize="18" 
						HorizontalOptions="StartAndExpand"/>
                        <Label  Text="{Binding lblTelephone}" 
						StyleClass="textsemibold"
						TextColor="{DynamicResource Primary}"
						FontSize="18" 
						HorizontalOptions="StartAndExpand"/>
                        <Label  Text="{Binding lblTelephoneVal}"
						StyleClass="textregular" 
						TextColor="{DynamicResource Primary}"
						FontSize="18" 
						HorizontalOptions="StartAndExpand"/>
                        <Label Text="{Binding lblFax}"
						StyleClass="textsemibold" 
						TextColor="{DynamicResource Primary}"
						FontSize="18" 
						HorizontalOptions="StartAndExpand"/>
                        <Label  Text="{Binding lblFaxVal}" 
						StyleClass="textregular"
						TextColor="{DynamicResource Primary}" 
						FontSize="18" 
						HorizontalOptions="StartAndExpand"/>
                        <Grid Margin="0,0,0,0">
                            <Grid.RowDefinitions>
                                <RowDefinition Height="60"/>
                                <RowDefinition Height="60"/>
                            </Grid.RowDefinitions>
                            <Button x:Name="btnModifyInformation"
							AutomationId="AbtnModifyInformation"
							Text="{Binding btnModifyInformation}" 
							StyleClass="Buttontextsmall,primarybuttoncolor,buttextregular" 
							CornerRadius="15"
							TextColor="White"
							Margin="0,10,0,0"
                               Grid.Row="0"   
					        VerticalOptions="FillAndExpand" 
                                    HorizontalOptions="Fill"
							Command="{Binding GotoModify}">
                            </Button>
                            <Button x:Name="btnSentOTP" 
							AutomationId="AbtnSentOTP"
							Text="{Binding btnSentOTP}"
							StyleClass="Buttontextsmall,primarybuttoncolor,buttextregular"
							CornerRadius="15"
							TextColor="White" 
							Margin="0,10,0,0" 
                                        Grid.Row="1"
                                    VerticalOptions="FillAndExpand"
                                    HorizontalOptions="Fill"
							Command="{Binding gotoOTP}">
                            </Button>
                        </Grid>
                    </StackLayout>
                    <StackLayout   IsVisible="{Binding Page6}" 
					Padding="30,0,30,0" 
					VerticalOptions="CenterAndExpand" >
                        <Grid>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="auto" />
                                <RowDefinition Height="auto" />
                                <RowDefinition Height="auto" />
                                <RowDefinition Height="auto" />
                                <RowDefinition Height="auto" />
                                <RowDefinition Height="auto" />
                            </Grid.RowDefinitions>
                       
                        <Image Grid.Row="0" Source="{Binding imgRegisterIconP6}"
							AutomationId="AimgRegisterIconP6"
							VerticalOptions="Start"
							HorizontalOptions="CenterAndExpand" 
							WidthRequest="100" 
							Margin="0,0,0,20"></Image>
                            <Label  Grid.Row="1" Text="{Binding lblCustomerRegistrationP6}" 
							FontFamily="Montserrat-Regular" 
							StyleClass="Heading1" 
							TextColor="{DynamicResource Primary}"
							FontSize="26"
							HorizontalOptions="CenterAndExpand"/>
                            <Label Grid.Row="2"  Text="{Binding lblEnterTheOtp}" 
							Margin="0,10,0,10" 
							FontFamily="Montserrat-Regular" 
							TextColor="{DynamicResource Primary}" 
							FontSize="18" 
							HorizontalTextAlignment="Center"
							HorizontalOptions="CenterAndExpand"/>
                            <Entry Grid.Row="3"  Text="{Binding TxtOtp}"
							AutomationId="ATxtOtp"
							Placeholder="{Binding PlaceOtp}" 
							FontFamily="Montserrat-Regular"
							PlaceholderColor="#b3b2b2"
							TextColor="{DynamicResource accprimarycolor}"
                                    BackgroundColor="{DynamicResource Entrytextbg}"
							Margin="0,0,0,0"></Entry>
                            <Label Grid.Row="4" Text="{Binding MsgValidOTP}" 
                                   IsVisible="{Binding IsvalidatedOTP}"
							FontFamily="Montserrat-Regular" 
							StyleClass="Heading1" 
							TextColor="Red"
							FontSize="18"
							HorizontalOptions="CenterAndExpand"/>
                            <Label Grid.Row="5" Opacity="0"></Label>
                        </Grid>
                        
                        <Grid VerticalOptions="StartAndExpand">
                            <Grid.RowDefinitions>
                                <RowDefinition Height="auto" />
                                <RowDefinition Height="auto" />
                            </Grid.RowDefinitions>
                            <Button  Text="{Binding btnConfirm}"
							AutomationId="AbtnConfirm"
							StyleClass="Buttontextsmall,primarybuttoncolor,buttextregular" 
							CornerRadius="15" 
							TextColor="White" 
							Margin="0,40,0,0" 
                            Grid.Row="0"
							Command="{Binding gotoPageRegistration7}"></Button>
                            <Button  Text="{Binding btnCancel}"
							AutomationId="AbtnCancel"
                            Grid.Row="1"
							StyleClass="Buttontextsmall,primarybuttoncolor,buttextregular" 
							CornerRadius="15" 
							TextColor="White"
							Margin="0,10,0,0" 
							Command="{Binding gotoPageFrom4}" ></Button>
                        </Grid>

                    </StackLayout>
                    <StackLayout IsVisible="{Binding Page7}" 
					Padding="30,0,30,0"
					Orientation="Vertical"
					VerticalOptions="CenterAndExpand">
                        <Grid>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="auto" />
                                <RowDefinition Height="auto" />
                                <RowDefinition Height="auto" />
                                <RowDefinition Height="auto" />
                                <RowDefinition Height="auto" />
                                <RowDefinition Height="auto" />
                                <RowDefinition Height="auto" />
                            </Grid.RowDefinitions>
                            <Image Grid.Row="0"  Source="{Binding imgRegisterIconP7}"
							AutomationId="AimgRegisterIconP7"
							VerticalOptions="Start"
							HorizontalOptions="CenterAndExpand" 
							WidthRequest="100" 
							Margin="0,0,0,20"></Image>
                            <Label Grid.Row="1" Text="{Binding lblDearCustomer}" 
							FontFamily="Montserrat-Regular"
							TextColor="{DynamicResource Primary}" 
							FontSize="24" 
							HorizontalOptions="CenterAndExpand"/>
                            <Label  Grid.Row="2" Text="{Binding lblMessage}"
							Margin="0,20,0,30" 
							FontFamily="Montserrat-Regular" 
							TextColor="{DynamicResource Primary}"
							FontSize="14" 
							HorizontalTextAlignment="Center"
							HorizontalOptions="CenterAndExpand"/>
                            <Button Grid.Row="3" x:Name="btnLogIn"
							AutomationId="AbtnLogIn"
							Text="{Binding btnLogIn}" 
							StyleClass="buttxt20,primarybuttoncolor,buttextregular" 
							CornerRadius="15"
							TextColor="White"
							Margin="0,0,0,0" 
							Command="{Binding gotoLogIn}"></Button>
                            <Label Grid.Row="4" Opacity="0"></Label>
                            <Label Grid.Row="5" Opacity="0"></Label>
                            <Label Grid.Row="6" Opacity="0"></Label>
                        </Grid>
                    </StackLayout>
                    <StackLayout IsVisible="{Binding PageFooter}"
					Padding="0,0,0,50"
                                 
					Orientation="Vertical"
					VerticalOptions="StartAndExpand">
                        <Grid   VerticalOptions="StartAndExpand">
                        <Grid.RowDefinitions>
                            <RowDefinition Height="auto" />
                            
                        </Grid.RowDefinitions>

                            <Button Grid.Row="0" x:Name="btnAlreadyLogIn"
						AutomationId="AbtnAlreadyLogIn"
						Text="{Binding btnAlreadyLogIn}" 
						StyleClass="Footerlinkregular"      
						BackgroundColor="Transparent" 
						TextColor="{DynamicResource Primary}"
						TextTransform="None" 
						HorizontalOptions="CenterAndExpand" 
                         VerticalOptions="StartAndExpand"
						Command="{Binding gotoLogIn}"></Button>
                       
                    </Grid>
                    </StackLayout>
                </StackLayout>
              
                
                <StackLayout  IsVisible="{Binding IsBusy}" 
							  AbsoluteLayout.LayoutBounds="0,0,1,1"
							  AbsoluteLayout.LayoutFlags="All" 
							  BackgroundColor="{Binding IndicatorBGColor}"
							  Opacity="{Binding IndicatorBGOpacity}">
                    <ActivityIndicator  IsRunning="{Binding IsBusy}"
										IsVisible="{Binding IsBusy}"
										HorizontalOptions="CenterAndExpand"
										VerticalOptions="StartAndExpand" 
										Color="{DynamicResource Primary}" />
                </StackLayout>
            </AbsoluteLayout>
        </ScrollView>
    </ContentPage.Content>
</ContentPage>