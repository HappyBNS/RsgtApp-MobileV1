<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:xct="http://xamarin.com/schemas/2020/toolkit"
             x:Name="Mycontainerdetail"
             xmlns:yummy="clr-namespace:Xamarin.Forms.PancakeView;assembly=Xamarin.Forms.PancakeView"
             x:Class="RsgtApp.Views.Containerdetail" 
			 FlowDirection="{DynamicResource pagedirection}" 
			 BackgroundImageSource="{DynamicResource Primarybgr}" 
			 NavigationPage.HasNavigationBar="True" 
			 NavigationPage.HasBackButton="True">
    <ContentPage.ToolbarItems>
        <!--<ToolbarItem Icon="{DynamicResource Langicon}" Priority="1" Order="Primary"/>-->
        <!--<ToolbarItem  Icon="notification_24.png" Priority="1" Order="Primary"/>-->
    </ContentPage.ToolbarItems>
    <ContentPage.Content>
        <AbsoluteLayout>
            <StackLayout IsEnabled="{Binding StackContainerDetails}" 
			             Orientation="Vertical" 
			             VerticalOptions="StartAndExpand" 
			             Spacing="0" 
			             Padding="10,0,10,0"  
						 IsVisible="true" 
						 AbsoluteLayout.LayoutBounds="0,0,1,1" 
						 AbsoluteLayout.LayoutFlags="All">
                <Grid RowSpacing="0" >
                <Grid.RowDefinitions>
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="*" />
                    <RowDefinition Height="Auto" />
                </Grid.RowDefinitions>
                    <StackLayout Spacing="0" 
					             VerticalOptions="StartAndExpand" 
								 Margin="0,0,0,10" 
								 Padding="0" 
								 Grid.Row="0" >
                        <Label Text="{Binding LblContainerDetails}" 
						       Margin="0,0,0,0" 
							   StyleClass="txt24,textsemibold" 
							   HorizontalTextAlignment="Center" 
							   TextColor="{DynamicResource Primary}" 
							   HorizontalOptions="Center">
                        </Label>
                    </StackLayout>
                <Frame Grid.Row="1" 
				       HasShadow="False" 
					   CornerRadius="20" 
					   Margin="0,0,0,0" 
					   VerticalOptions="EndAndExpand" 
					   Padding="20">
                    <Frame.Background>
                        <LinearGradientBrush StartPoint="1,10" EndPoint="1,0">
                            <GradientStop Color="{DynamicResource frameprimary}" Offset="0.9"/>
                            <GradientStop Color="{DynamicResource frameprimary}" Offset="1.0"/>
                        </LinearGradientBrush>
                    </Frame.Background>
                    <ScrollView >
                        <Frame BackgroundColor="Transparent" 
						       HasShadow="False" 
							   xct:ShadowEffect.Radius="20"  
							   CornerRadius="20" 
							   Padding="5" 
							   Margin="0,0,0,0">
                            <StackLayout Orientation="Vertical" 
							             VerticalOptions="StartAndExpand" 
										 Spacing="0">
                                    <Label Text="{Binding LblContainerNo}" 
									       Margin="0,0,0,0" 
										   StyleClass="txt22,textsemibold" 
										   HorizontalTextAlignment="Start" 
										   TextColor="{DynamicResource captioncolor}" 
										   HorizontalOptions="StartAndExpand"/>
                                    <Label Text="{Binding StrContainerNo}" 
									       Margin="0,0,0,0" 
										   StyleClass="txt22,textregular" 
										   HorizontalTextAlignment="Start" 
										   TextColor="{DynamicResource infocolor}" 
										   HorizontalOptions="StartAndExpand"/>
                                <StackLayout Orientation="Vertical" Margin="0,20,0,0">
                                    <StackLayout Orientation="Horizontal" Margin="0">
                                            <Image Source="{Binding ImgVisit}" 
											       HorizontalOptions="Start" 
												   VerticalOptions="Center" 
												   HeightRequest="24" 
												   Margin="0,0,0,0"/>
                                            <Label Text="{Binding LblVesselVisitId}" 
											       Margin="0,0,0,0" 
												   StyleClass="txt16,textsemibold" 
												   HorizontalTextAlignment="Start" 
												   TextColor="{DynamicResource captioncolor}" 
												   HorizontalOptions="Start"/>
                                    </StackLayout>
                                        <Label Text="{Binding StrVesselVisitId}" 
										        Margin="0,0,0,0" 
												StyleClass="txt16,textregular" 
												HorizontalTextAlignment="Start" 
												TextColor="{DynamicResource infocolor}" 
												HorizontalOptions="StartAndExpand"/>
                                </StackLayout>
                                <StackLayout Orientation="Vertical" Margin="0,20,0,0">
                                    <StackLayout Orientation="Horizontal" Margin="0">
                                            <Image Source="{Binding ImgShippingLine3}" 
											       HorizontalOptions="Start" 
												   VerticalOptions="Center" 
												   HeightRequest="24" 
												   Margin="0,0,0,0"/>
                                            <Label Text="{Binding LblVeeselName}" 
											       Margin="0,0,0,0" 
												   StyleClass="txt16,textsemibold" 
												   HorizontalTextAlignment="Start" 
												   TextColor="{DynamicResource captioncolor}" 
												   HorizontalOptions="Start"/>
                                    </StackLayout>
                                        <Label Text="{Binding StrVeeselName}" 
										       Margin="0,0,0,0" 
											   StyleClass="txt16,textregular" 
											   HorizontalTextAlignment="Start" 
											   TextColor="{DynamicResource infocolor}" 
											   HorizontalOptions="StartAndExpand"/>
                                </StackLayout>
                                <StackLayout Orientation="Vertical" Margin="0,20,0,0">
                                    <StackLayout Orientation="Horizontal" Margin="0">
                                            <Image Source="{Binding ImgVoyage}" 
											       HorizontalOptions="Start" 
												   VerticalOptions="Center" 
												   HeightRequest="24" 
												   Margin="0,0,0,0"/>
                                            <Label Text="{Binding LblVoyage}" 
