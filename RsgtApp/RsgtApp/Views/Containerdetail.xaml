												   VerticalOptions="Center" 
												   HeightRequest="24" 
												   Margin="0,0,0,0"/>
                                            <Label Text="{Binding LblPortOfDischarge }" 
											       Margin="0,0,0,0" 
												   StyleClass="txt16,textsemibold" 
												   HorizontalTextAlignment="Start" 
