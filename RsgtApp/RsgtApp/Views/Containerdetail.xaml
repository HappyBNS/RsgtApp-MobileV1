                                                    <Grid.ColumnDefinitions>
                                                        <ColumnDefinition Width="20"/>
                                                        <ColumnDefinition Width="Auto"/>
                                                    </Grid.ColumnDefinitions>
                                                </Grid>
                                                <StackLayout Grid.Column="0" WidthRequest="15" >
                                                    <BoxView Color="#93d6e7" 
											         WidthRequest="1" 
											         HorizontalOptions="Center" 
													 VerticalOptions="FillAndExpand"/>
                                                </StackLayout>
                                                <Label Text="{Binding LblValGatedIn}" 
											       StyleClass="Statusfont2, Statustext2" 
												   VerticalOptions="Start" 
												   HorizontalOptions="StartAndExpand" 
												   Grid.Column="1" />
                                            </StackLayout>
                                        </Grid>
                                        <!--Customs Inspection-->
                                        <Grid RowSpacing="0" Margin="0">
                                            <Grid.RowDefinitions>
                                                <RowDefinition Height="Auto"/>
                                                <RowDefinition Height="Auto"/>
                                            </Grid.RowDefinitions>
                                            <StackLayout Orientation="Horizontal" 
                                                         Margin="0,0,0,0" 
                                                         Grid.Row="1">
                                                <Grid>
                                                    <Grid.ColumnDefinitions>
                                                        <ColumnDefinition Width="20"/>
                                                        <ColumnDefinition Width="Auto"/>
                                                    </Grid.ColumnDefinitions>
                                                </Grid>
                                                <Image Source="{Binding ImgexpCustomsInspection}" 
                                                       WidthRequest="15" 
                                                       Grid.Column="0"/>
                                                <Label Text="{Binding LblCustomsInspection}"
											       StyleClass="Statusfont1, Statustext1" 
												   VerticalOptions="StartAndExpand" 
												   HorizontalOptions="StartAndExpand" 
												   Grid.Column="2" />
                                            </StackLayout>
                                            <StackLayout Orientation="Horizontal"  
                                                         Margin="0,0,0,0" 
                                                         Grid.Row="2">
                                                <Grid>
                                                    <Grid.ColumnDefinitions>
                                                        <ColumnDefinition Width="20"/>
                                                        <ColumnDefinition Width="Auto"/>
                                                    </Grid.ColumnDefinitions>
                                                </Grid>
                                                <StackLayout Grid.Column="0" 
                                                             WidthRequest="15" >
                                                    <BoxView Color="#93d6e7" 
											         WidthRequest="1" 
													 HorizontalOptions="Center" 
													 VerticalOptions="FillAndExpand"/>
                                                </StackLayout>
                                                <Label Text="{Binding LblValCustomsInspection}" 
                                                       IsVisible="{Binding isVisibleValInspection}"
											       StyleClass="Statusfont2, Statustext2" 
												   VerticalOptions="Start" 
												   HorizontalOptions="StartAndExpand" 
												   Grid.Column="1" />
                                                <Label Text="{Binding LblCustomsInspectionMsg}" 
                                                       IsVisible="{Binding isVisibleInspection}"
											       StyleClass="Statusfont2, Statustext2" 
												   VerticalOptions="Start" 
												   HorizontalOptions="StartAndExpand" 
												   Grid.Column="1" />
                                            </StackLayout>
                                        </Grid>
                                        <!--Col Excess Cargo-->
                                        <Grid RowSpacing="0">
                                            <Grid.RowDefinitions>
                                                <RowDefinition Height="Auto"/>
                                                <RowDefinition Height="Auto"/>
                                            </Grid.RowDefinitions>
                                            <StackLayout Orientation="Horizontal"  
                                                         Margin="0,0,0,0" 
                                                         Grid.Row="1">
                                                <Grid>
                                                    <Grid.ColumnDefinitions>
                                                        <ColumnDefinition Width="20"/>
                                                        <ColumnDefinition Width="Auto"/>
                                                    </Grid.ColumnDefinitions>
                                                </Grid>

                                                <Image Source="{Binding ImgexpExcessCargo}" 
                                                       WidthRequest="15" 
                                                       Grid.Column="0"/>
                                                <Label Text="{Binding LblColExcessCargo}" 
											StyleClass="Statusfont1, Statustext1" 
											VerticalOptions="CenterAndExpand" 
											HorizontalOptions="StartAndExpand" 
											Grid.Column="1"  />
                                            </StackLayout>
                                            <StackLayout Orientation="Horizontal"  
                                                         Margin="0,0,0,0" 
                                                         Grid.Row="2">
                                                <Grid>
                                                    <Grid.ColumnDefinitions>
                                                        <ColumnDefinition Width="20"/>
                                                        <ColumnDefinition Width="Auto"/>
                                                    </Grid.ColumnDefinitions>
                                                </Grid>
                                                <StackLayout Grid.Column="0" WidthRequest="15" >
                                                    <BoxView Color="#93d6e7" 
											         WidthRequest="1" 
													 HorizontalOptions="Center" 
													 VerticalOptions="FillAndExpand"/>
                                                </StackLayout>
                                                <Label Text="{Binding LblValColExcessCargo}" 
											       StyleClass="Statusfont2, Statustext2" 
												   VerticalOptions="Start" 
												   HorizontalOptions="StartAndExpand"
												   Grid.Column="1" />
                                                <Label Text="{Binding cd_containerdocument}" 
                                                       IsVisible="False"/>
                                            </StackLayout>
                                        </Grid>
                                        <!--Wait Ship line-->
                                        <Grid RowSpacing="0">
                                            <Grid.RowDefinitions>
                                                <RowDefinition Height="Auto"/>
                                                <RowDefinition Height="Auto"/>
                                            </Grid.RowDefinitions>
                                            <StackLayout Orientation="Horizontal"  Margin="0,0,0,0" Grid.Row="1">
                                                <Grid>
                                                    <Grid.ColumnDefinitions>
                                                        <ColumnDefinition Width="20"/>
                                                        <ColumnDefinition Width="Auto"/>
                                                    </Grid.ColumnDefinitions>
                                                </Grid>
                                                <Image Source="{Binding ImgexpWaitShipline}" 
                                                       WidthRequest="15" 
                                                       Grid.Column="0"/>
                                                <Label Text="{Binding LblWaitShipline}" 
											       StyleClass="Statusfont1, Statustext1" 
												   VerticalOptions="CenterAndExpand" 
												   HorizontalOptions="StartAndExpand" 
												   Grid.Column="1" />
                                            </StackLayout>
                                            <StackLayout Orientation="Horizontal"  
									             Margin="0,0,0,0" 
												 Grid.Row="2">
                                                <Grid>
                                                    <Grid.ColumnDefinitions>
                                                        <ColumnDefinition Width="20"/>
                                                        <ColumnDefinition Width="Auto"/>
                                                    </Grid.ColumnDefinitions>
                                                </Grid>
                                                <StackLayout Grid.Column="0" 
                                                             WidthRequest="15" >
                                                    <BoxView Color="#93d6e7" 
                                                             WidthRequest="1" 
                                                             HorizontalOptions="Center" 
                                                             VerticalOptions="FillAndExpand"/>
                                                </StackLayout>
                                                <Label  Text="{Binding LblValWaitShipline}" 
											        StyleClass="Statusfont2, Statustext2" 
													VerticalOptions="Start" 
													HorizontalOptions="StartAndExpand" 
													Grid.Column="1" />
                                            </StackLayout>
                                        </Grid>
                                        <!--Wait cold store payment-->
                                        <Grid RowSpacing="0">
                                            <Grid.RowDefinitions>
                                                <RowDefinition Height="Auto"/>
                                                <RowDefinition Height="Auto"/>
                                            </Grid.RowDefinitions>
                                            <StackLayout Orientation="Horizontal"  Margin="0,0,0,0" Grid.Row="1">
                                                <Grid>
                                                    <Grid.ColumnDefinitions>
                                                        <ColumnDefinition Width="20"/>
                                                        <ColumnDefinition Width="Auto"/>
                                                    </Grid.ColumnDefinitions>
                                                </Grid>
                                                <Image Source="{Binding ImgexpWaitcoldstorepayment}" 
                                                       WidthRequest="15" 
                                                       Grid.Column="0"/>
                                                <Label Text="{Binding LblWaitColdstorepayment}" 
											       StyleClass="Statusfont1, Statustext1" 
												   VerticalOptions="CenterAndExpand" 
												   HorizontalOptions="StartAndExpand" 
												   Grid.Column="1" />
                                            </StackLayout>
                                            <StackLayout Orientation="Horizontal"  
                                                         Margin="0,0,0,0" 
                                                         Grid.Row="2">
                                                <Grid>
                                                    <Grid.ColumnDefinitions>
                                                        <ColumnDefinition Width="20"/>
                                                        <ColumnDefinition Width="Auto"/>
                                                    </Grid.ColumnDefinitions>
                                                </Grid>
                                                <StackLayout Grid.Column="0" 
                                                             WidthRequest="15" >
                                                    <BoxView Color="#93d6e7" 
											         WidthRequest="1" 
													 HorizontalOptions="Center" 
													 VerticalOptions="FillAndExpand"/>
                                                </StackLayout>
                                                <Label Text="{Binding LblValWaitcoldstorepayment}" 
											       StyleClass="Statusfont2, Statustext2" 
												   VerticalOptions="Start" 
												   HorizontalOptions="StartAndExpand" 
												   Grid.Column="1" />
                                            </StackLayout>
                                        </Grid>
                                        <!--Wait for RSGT Payment-->
                                        <Grid RowSpacing="0">
                                            <Grid.RowDefinitions>
                                                <RowDefinition Height="Auto"/>
                                                <RowDefinition Height="Auto"/>
                                            </Grid.RowDefinitions>
                                            <StackLayout Orientation="Horizontal"  Margin="0,0,0,0" Grid.Row="1">
                                                <Grid>
                                                    <Grid.ColumnDefinitions>
                                                        <ColumnDefinition Width="20"/>
                                                        <ColumnDefinition Width="Auto"/>
                                                    </Grid.ColumnDefinitions>
                                                </Grid>
                                                <Image Source="{Binding ImgexpWaitforRSGTPayment}" 
                                                       WidthRequest="15" 
                                                       Grid.Column="0"/>
                                                <Label Text="{Binding LblWaitforRSGTPayment}" 
											       StyleClass="Statusfont1, Statustext1" 
												   VerticalOptions="CenterAndExpand" 
												   HorizontalOptions="StartAndExpand" 
												   Grid.Column="1" />
                                            </StackLayout>
                                            <StackLayout Orientation="Horizontal"  
                                                         Margin="0,0,0,0" 
                                                         Grid.Row="2">
                                                <Grid>
                                                    <Grid.ColumnDefinitions>
                                                        <ColumnDefinition Width="20"/>
                                                        <ColumnDefinition Width="Auto"/>
                                                    </Grid.ColumnDefinitions>
                                                </Grid>
                                                <StackLayout Grid.Column="0" 
                                                             WidthRequest="15" >
                                                    <BoxView Color="#93d6e7" 
                                                             WidthRequest="1" 
											         HorizontalOptions="Center" 
													 VerticalOptions="FillAndExpand"/>
                                                </StackLayout>
                                                <Label  Text="{Binding LblValWaitforRSGTPayment}" 
											        StyleClass="Statusfont2, Statustext2" 
													VerticalOptions="Start" 
													HorizontalOptions="StartAndExpand" 
													Grid.Column="1" />
                                            </StackLayout>
                                        </Grid>
                                        <!--Ready to Load-->
                                        <Grid RowSpacing="0">
                                            <Grid.RowDefinitions>
                                                <RowDefinition Height="Auto"/>
                                                <RowDefinition Height="Auto"/>
                                            </Grid.RowDefinitions>
                                            <StackLayout Orientation="Horizontal"  Margin="0,0,0,0" Grid.Row="1">
                                                <Grid>
                                                    <Grid.ColumnDefinitions>
                                                        <ColumnDefinition Width="20"/>
                                                        <ColumnDefinition Width="Auto"/>
                                                    </Grid.ColumnDefinitions>
                                                </Grid>
                                                <Image Source="{Binding ImgexpReadytoLoad}" 
                                                       WidthRequest="15" 
                                                       Grid.Column="0"/>
                                                <Label Text="{Binding LblReadytoload}" 
											       StyleClass="Statusfont1, Statustext1" 
												   VerticalOptions="CenterAndExpand" 
												   HorizontalOptions="StartAndExpand" 
												   Grid.Column="1" />
                                            </StackLayout>
                                            <StackLayout Orientation="Horizontal"  
                                                         Margin="0,0,0,0" 
                                                         Grid.Row="2">
                                                <Grid>
                                                    <Grid.ColumnDefinitions>
                                                        <ColumnDefinition Width="20"/>
                                                        <ColumnDefinition Width="Auto"/>
                                                    </Grid.ColumnDefinitions>
                                                </Grid>
                                                <StackLayout Grid.Column="0" 
                                                             WidthRequest="15" >
                                                    <BoxView Color="#93d6e7" 
											         WidthRequest="1" 
													 HorizontalOptions="Center" 
													 VerticalOptions="FillAndExpand"/>
                                                </StackLayout>
                                                <Label  Text="{Binding LblValReadytoload}"
											        StyleClass="Statusfont2, Statustext2" 
													VerticalOptions="Start" 
													HorizontalOptions="StartAndExpand" 
													Grid.Column="1" />
                                            </StackLayout>
                                        </Grid>
                                        <!--Loaded on vessel-->
                                        <Grid RowSpacing="0">
                                            <Grid.RowDefinitions>
                                                <RowDefinition Height="Auto"/>
                                                <RowDefinition Height="Auto"/>
                                            </Grid.RowDefinitions>
                                            <StackLayout Orientation="Horizontal"  
                                                         Margin="0,0,0,0" 
                                                         Grid.Row="1">
                                                <Grid>
                                                    <Grid.ColumnDefinitions>
                                                        <ColumnDefinition Width="20"/>
                                                        <ColumnDefinition Width="Auto"/>
                                                    </Grid.ColumnDefinitions>
                                                </Grid>
                                                <Image Source="{Binding ImgexpLoadedonvessel}" 
                                                       WidthRequest="15" 
                                                       Grid.Column="0"/>
                                                <Label Text="{Binding LblLoadedonVessel}" 
											       StyleClass="Statusfont1, Statustext1" 
												   VerticalOptions="CenterAndExpand" 
												   HorizontalOptions="StartAndExpand" 
												   Grid.Column="1" />
                                            </StackLayout>
                                            <StackLayout Orientation="Horizontal" 
                                                         Margin="0,0,0,0" 
                                                         Grid.Row="2">
                                                <Grid>
                                                    <Grid.ColumnDefinitions>
                                                        <ColumnDefinition Width="20"/>
                                                        <ColumnDefinition Width="Auto"/>
                                                    </Grid.ColumnDefinitions>
                                                </Grid>
                                                <StackLayout Grid.Column="0" 
                                                             WidthRequest="15" >
                                                    <BoxView Color="#93d6e7"
                                                             WidthRequest="0"
                                                             HorizontalOptions="Center" 
                                                             VerticalOptions="FillAndExpand"/>
                                                </StackLayout>
                                                <Label Text="{Binding LblValLoadedonvessel}"
                                                       IsVisible="{Binding isVisibleValLoadedVessel}"
											       StyleClass="Statusfont2, Statustext2" 
												   VerticalOptions="Start" 
												   HorizontalOptions="StartAndExpand" 
												   Grid.Column="1" />
                                                <Label Text="{Binding LblLoadedonvesselMsg}" 
                                                       IsVisible="{Binding isVisibleLoadedVessel}"
											       StyleClass="Statusfont2, Statustext2" 
												   VerticalOptions="Start" 
												   HorizontalOptions="StartAndExpand" 
												   Grid.Column="1" />
                                            </StackLayout>
                                        </Grid>
                                    </StackLayout>
                                    <!--Survey-->
                                    <!--Survey-->
                                    <Grid RowSpacing="0">
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="Auto"/>
                                            <RowDefinition Height="Auto"/>
                                        </Grid.RowDefinitions>
                                        <StackLayout Orientation="Horizontal"  Margin="0,0,0,0" Grid.Row="1">
                                            <Grid>
                                                <Grid.ColumnDefinitions>
                                                    <ColumnDefinition Width="20"/>
                                                    <ColumnDefinition Width="Auto"/>
                                                </Grid.ColumnDefinitions>
                                            </Grid>
                                            <Label Text="{Binding Lblsurvey}" 
											       StyleClass="Statusfont1, Statustext1" 
												   VerticalOptions="StartAndExpand" 
												   HorizontalOptions="StartAndExpand" 
												   Grid.Column="1" />
                                        </StackLayout>
                                        <StackLayout Orientation="Horizontal"  Margin="0,0,0,0" Grid.Row="2" >
                                            <Grid>
                                                <Grid.ColumnDefinitions>
                                                    <ColumnDefinition Width="20"/>
                                                    <ColumnDefinition Width="Auto"/>
                                                </Grid.ColumnDefinitions>
                                            </Grid>
                                            <!--<Label Text="{Binding LblDearConsignee}" 
											        AutomationId="ALblDearConsignee"
											        StyleClass="Statusfont2, Statustext2" 
													VerticalOptions="CenterAndExpand" 
													HorizontalOptions="StartAndExpand" 
													Grid.Column="1" 
													 />-->

                                            <Label Text="{Binding LblDearConsignee}" 
                                                               StyleClass="Statusfont2, Statustext2" 
                                                               VerticalOptions="StartAndExpand" 
                                                               HorizontalOptions="StartAndExpand"
                                                               Grid.Column="1">
                                                <Label.GestureRecognizers>
                                                    <TapGestureRecognizer 
                                                                    Command="{Binding Source={x:Reference Mycontainerdetail},
                                                                    Path=BindingContext.gotosurvey}" 
                                                                    NumberOfTapsRequired="1"/>
                                                </Label.GestureRecognizers>
                                            </Label>
                                        </StackLayout>
                                    </Grid>
                                </StackLayout>
                        </Frame>
                    </ScrollView>
                </Frame>
                    <StackLayout VerticalOptions="StartAndExpand" 
                             Margin="0,0,0,10" 
                             Padding="0" 
                             Grid.Row="2" >
                        <Grid>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="*" />
                                <ColumnDefinition Width="*" />
                            </Grid.ColumnDefinitions>
                            <Frame BackgroundColor="{DynamicResource Searchframe}" 
						       HasShadow="False" 
							   CornerRadius="15" 
							   HeightRequest="55" 
							   Margin="0,10,0,0" 
							   Padding="15,0,15,0" 
							   Grid.Column="0">
                                <ImageButton Source="email_icon.png" 
								             Padding="15" 
											 HeightRequest="20" 
											 WidthRequest="20" 
											 BackgroundColor="Transparent" 
											 Grid.Column="2" 
											 Command="{Binding ContainerPDFMail}"
											 AutomationId="AContainerPDFMail" />
                            </Frame>
                            <Frame BackgroundColor="{DynamicResource Searchframe}" 
						       HasShadow="False" 
							   CornerRadius="15" 
							   HeightRequest="55" 
							   Margin="0,10,0,0" 
							   Padding="15,0,15,0" 
							   Grid.Column="1">
                                <ImageButton Source="print_icon.png" 
								             Padding="15" 
											 WidthRequest="10" 
											 BackgroundColor="Transparent" 
											 Grid.Column="2" 
											 Command="{Binding ContainerPDFprint}"
											  AutomationId="AContainerPDFprint"	/>
                            </Frame>
                        </Grid>
                    </StackLayout>
                </Grid>
        </StackLayout>
            <StackLayout IsVisible="{Binding IsBusy}"  
                         AbsoluteLayout.LayoutBounds="0,0,1,1" 
                         AbsoluteLayout.LayoutFlags="All" 
                         BackgroundColor="{Binding IndicatorBGColor}"  
                         Opacity="{Binding IndicatorBGOpacity}">
                <ActivityIndicator IsVisible="{Binding IsBusy}" 
                        IsRunning="{Binding IsBusy}"  
                        HorizontalOptions="CenterAndExpand" 
                        VerticalOptions="CenterAndExpand" 
                        Color="{DynamicResource Primary}" />
            </StackLayout>
        </AbsoluteLayout>
    </ContentPage.Content>
</ContentPage>