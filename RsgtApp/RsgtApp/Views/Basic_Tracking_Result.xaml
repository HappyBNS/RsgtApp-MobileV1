                                               StyleClass="Statusfont1, Statustext1" 
                                               VerticalOptions="CenterAndExpand" 
                                               HorizontalOptions="StartAndExpand" 
                                               Grid.Column="1"></Label>
                                        </StackLayout>
                                        <StackLayout Orientation="Horizontal" 
                                                     Margin="0,0,0,0" Grid.Row="2">
                                            <Grid>
                                                <Grid.ColumnDefinitions>
                                                    <ColumnDefinition Width="20"/>
                                                    <ColumnDefinition Width="Auto"/>
                                                </Grid.ColumnDefinitions>
                                            </Grid>
                                            <StackLayout Grid.Column="0" 
                                                         WidthRequest="15" >
                                                <BoxView Color="#93d6e7" 
                                                     WidthRequest="1" 
                                                     HorizontalOptions="Center" 
                                                     VerticalOptions="FillAndExpand"/>
                                            </StackLayout>
                                            <Label 
                                               Text="{Binding UnderInspection}" 
                                               StyleClass="Statusfont2, Statustext2" 
                                               VerticalOptions="Start" 
                                               HorizontalOptions="StartAndExpand" 
                                               Grid.Column="1"></Label>
                                        </StackLayout>
                                    </Grid>
                                    <Grid RowSpacing="0">
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="Auto"/>
                                            <RowDefinition Height="Auto"/>
                                        </Grid.RowDefinitions>
                                        <StackLayout Orientation="Horizontal" 
                                                     Margin="0,0,0,0"
                                                     Grid.Row="1">
                                            <Grid>
                                                <Grid.ColumnDefinitions>
                                                    <ColumnDefinition Width="20"/>
                                                    <ColumnDefinition Width="Auto"/>
                                                </Grid.ColumnDefinitions>
                                            </Grid>
                                            <Image 
                                               Source="{Binding ImgStatusIconInsComp}" 
                                               WidthRequest="15" 
                                               Grid.Column="0"/>
                                            <Label 
                                               Text="{Binding LblInspectionCompleted}" 
                                               StyleClass="Statusfont1, Statustext1" 
                                               VerticalOptions="CenterAndExpand" 
                                               HorizontalOptions="StartAndExpand" 
                                               Grid.Column="1" ></Label>
                                        </StackLayout>
                                        <StackLayout Orientation="Horizontal"  
                                                     Margin="0,0,0,0" 
                                                     Grid.Row="2">
                                            <Grid>
                                                <Grid.ColumnDefinitions>
                                                    <ColumnDefinition Width="20"/>
                                                    <ColumnDefinition Width="Auto"/>
                                                </Grid.ColumnDefinitions>
                                            </Grid>
                                            <StackLayout Grid.Column="0" 
                                                         WidthRequest="15" >
                                                <BoxView Color="#93d6e7" 
                                                         WidthRequest="1" 
                                                         HorizontalOptions="Center" 
                                                         VerticalOptions="FillAndExpand"/>
                                            </StackLayout>
                                            <Label  Text="{Binding InspectionCompleted}"
                                                    StyleClass="Statusfont2, Statustext2"
                                                    VerticalOptions="Start" 
                                                    HorizontalOptions="StartAndExpand" 
                                                    Grid.Column="1" ></Label>
                                        </StackLayout>
                                    </Grid>
                                    <Grid RowSpacing="0" IsVisible="false">
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="Auto"/>
                                            <RowDefinition Height="Auto"/>
                                        </Grid.RowDefinitions>
                                        <StackLayout Orientation="Horizontal" 
                                                     Margin="0,0,0,0"
                                                     Grid.Row="1">
                                            <Grid>
                                                <Grid.ColumnDefinitions>
                                                    <ColumnDefinition Width="20"/>
                                                    <ColumnDefinition Width="Auto"/>
                                                </Grid.ColumnDefinitions>
                                            </Grid>
                                            <Image 
                                               Source="{Binding ImgStatusGreenIcon4}" 
                                               WidthRequest="15" 
                                               Grid.Column="0"/>
                                            <Label  
                                               Text="{Binding LblContactShippingLine}" 
                                               StyleClass="Statusfont1, Statustext1" 
                                               VerticalOptions="CenterAndExpand" 
                                               HorizontalOptions="StartAndExpand" 
                                               Grid.Column="1" ></Label>
                                        </StackLayout>
                                        <StackLayout Orientation="Horizontal" 
                                                     Margin="0,0,0,0"
                                                     Grid.Row="2">
                                            <Grid>
                                                <Grid.ColumnDefinitions>
                                                    <ColumnDefinition Width="20"/>
                                                    <ColumnDefinition Width="Auto"/>
                                                </Grid.ColumnDefinitions>
                                            </Grid>
                                            <StackLayout Grid.Column="0"
                                                         WidthRequest="15" >
                                                <BoxView Color="#93d6e7" 
                                                     WidthRequest="1" 
                                                     HorizontalOptions="Center" 
                                                     VerticalOptions="FillAndExpand"/>
                                            </StackLayout>
                                            <Label 
                                               Text="{Binding ContactShippingLine}" 
                                               StyleClass="Statusfont2, Statustext2" 
                                               VerticalOptions="Start" 
                                               HorizontalOptions="StartAndExpand" 
                                               Grid.Column="1"></Label>
                                        </StackLayout>
                                    </Grid>
                                    <Grid RowSpacing="0">
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="Auto"/>
                                            <RowDefinition Height="Auto"/>
                                        </Grid.RowDefinitions>
                                        <StackLayout Orientation="Horizontal" 
                                                     Margin="0,0,0,0"
                                                     Grid.Row="1">
                                            <Grid>
                                                <Grid.ColumnDefinitions>
                                                    <ColumnDefinition Width="20"/>
                                                    <ColumnDefinition Width="Auto"/>
                                                </Grid.ColumnDefinitions>
                                            </Grid>
                                            <Image 
                                               Source="{Binding ImgStatusIconRFD2}" 
                                               WidthRequest="15" 
                                               Grid.Column="0"/>
                                            <Label  
                                               Text="{Binding Lblportcharge}" 
                                               StyleClass="Statusfont1, Statustext1"
                                               VerticalOptions="CenterAndExpand" 
                                               HorizontalOptions="StartAndExpand" 
                                               Grid.Column="1" ></Label>
                                        </StackLayout>
                                        <StackLayout Orientation="Horizontal" 
                                                     Margin="0,0,0,0" 
                                                     Grid.Row="2">
                                            <Grid>
                                                <Grid.ColumnDefinitions>
                                                    <ColumnDefinition Width="20"/>
                                                    <ColumnDefinition Width="Auto"/>
                                                </Grid.ColumnDefinitions>
                                            </Grid>
                                            <StackLayout Grid.Column="0" WidthRequest="15" >
                                                <BoxView Color="#93d6e7" 
                                                     WidthRequest="1" 
                                                     HorizontalOptions="Center" 
                                                     VerticalOptions="FillAndExpand"/>
                                            </StackLayout>
                                            <Label 
                                               Text="{Binding WaitingForPortcharge}" 
                                               StyleClass="Statusfont2, Statustext2" 
                                               VerticalOptions="Start" 
                                               HorizontalOptions="StartAndExpand" 
                                               Grid.Column="1" ></Label>
                                        </StackLayout>
                                    </Grid>
                                    <Grid RowSpacing="0">
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="Auto"/>
                                            <RowDefinition Height="Auto"/>
                                        </Grid.RowDefinitions>
                                        <StackLayout Orientation="Horizontal" 
                                                     Margin="0,0,0,0"
                                                     Grid.Row="1">
                                            <Grid>
                                                <Grid.ColumnDefinitions>
                                                    <ColumnDefinition Width="20"/>
                                                    <ColumnDefinition Width="Auto"/>
                                                </Grid.ColumnDefinitions>
                                            </Grid>
                                            <Image 
                                               Source="{Binding ImgStatusIconRFD}" 
                                               WidthRequest="15" 
                                               Grid.Column="0"/>
                                            <Label 
                                               Text="{Binding LblReadForDelivery}" 
                                               StyleClass="Statusfont1, Statustext1" 
                                               VerticalOptions="CenterAndExpand" 
                                               HorizontalOptions="StartAndExpand" 
                                               Grid.Column="1" ></Label>
                                        </StackLayout>
                                        <StackLayout Orientation="Horizontal" 
                                                     Margin="0,0,0,0" 
                                                     Grid.Row="2">
                                            <Grid>
                                                <Grid.ColumnDefinitions>
                                                    <ColumnDefinition Width="20"/>
                                                    <ColumnDefinition Width="Auto"/>
                                                </Grid.ColumnDefinitions>
                                            </Grid>
                                            <StackLayout Grid.Column="0" 
                                                         WidthRequest="15" >
                                                <BoxView Color="#93d6e7" 
                                                     WidthRequest="1" 
                                                     HorizontalOptions="Center" 
                                                     VerticalOptions="FillAndExpand"/>
                                            </StackLayout>
                                            <Label 
                                               Text="{Binding RFD}" 
                                               StyleClass="Statusfont2, Statustext2" 
                                               VerticalOptions="Start" 
                                               HorizontalOptions="StartAndExpand" 
                                               Grid.Column="1" ></Label>
                                        </StackLayout>
                                    </Grid>
                                    <Grid RowSpacing="0">
                                        <Grid.RowDefinitions>

                                            <RowDefinition Height="Auto"/>
                                            <RowDefinition Height="Auto"/>
                                        </Grid.RowDefinitions>
                                        <StackLayout Orientation="Horizontal" 
                                                     Margin="0,0,0,0"
                                                     Grid.Row="1">
                                            <Grid>
                                                <Grid.ColumnDefinitions>
                                                    <ColumnDefinition Width="20"/>
                                                    <ColumnDefinition Width="Auto"/>
                                                </Grid.ColumnDefinitions>
                                            </Grid>
                                            <Image 
                                               Source="{Binding ImgStatusIconPrePickUp}" 
                                               WidthRequest="15" 
                                               Grid.Column="0"/>
                                            <Label   
                                               Text="{Binding LblPrePickUpIssued}" 
                                               StyleClass="Statusfont1, Statustext1" 
                                               VerticalOptions="CenterAndExpand" 
                                               HorizontalOptions="StartAndExpand" 
                                               Grid.Column="1" ></Label>
                                        </StackLayout>
                                        <StackLayout Orientation="Horizontal"  
                                                     Margin="0,0,0,0" 
                                                     Grid.Row="2">
                                            <Grid>
                                                <Grid.ColumnDefinitions>
                                                    <ColumnDefinition Width="20"/>
                                                    <ColumnDefinition Width="Auto"/>
                                                </Grid.ColumnDefinitions>
                                            </Grid>
                                            <StackLayout Grid.Column="0"
                                                         WidthRequest="15" >
                                                <BoxView Color="#93d6e7" 
                                                     WidthRequest="1" 
                                                     HorizontalOptions="Center" 
                                                     VerticalOptions="FillAndExpand"/>
                                            </StackLayout>
                                            <Label   
                                               Text="{Binding PrePickUp}" 
                                               StyleClass="Statusfont2, Statustext2" 
                                               VerticalOptions="Start" 
                                               HorizontalOptions="StartAndExpand" 
                                               Grid.Column="1"></Label>
                                        </StackLayout>
                                    </Grid>
                                    <Grid RowSpacing="0">
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="Auto"/>
                                            <RowDefinition Height="Auto"/>
                                        </Grid.RowDefinitions>
                                        <StackLayout Orientation="Horizontal"
                                                     Margin="0,0,0,0" 
                                                     Grid.Row="1">
                                            <Grid>
                                                <Grid.ColumnDefinitions>
                                                    <ColumnDefinition Width="20"/>
                                                    <ColumnDefinition Width="Auto"/>
                                                </Grid.ColumnDefinitions>
                                            </Grid>
                                            <Image 
                                               Source="{Binding ImgStatusIconGO}" 
                                               WidthRequest="15" 
                                               Grid.Column="0"/>
                                            <Label  
                                               Text="{Binding LblGatedOut}" 
                                               StyleClass="Statusfont1, Statustext1" 
                                               VerticalOptions="CenterAndExpand" 
                                               HorizontalOptions="StartAndExpand" 
                                               Grid.Column="1" ></Label>
                                        </StackLayout>
                                        <StackLayout Orientation="Horizontal"  
                                                     Margin="0,0,0,0" 
                                                     Grid.Row="2">
                                            <Grid>
                                                <Grid.ColumnDefinitions>
                                                    <ColumnDefinition Width="20"/>
                                                    <ColumnDefinition Width="Auto"/>
                                                </Grid.ColumnDefinitions>
                                            </Grid>
                                            <StackLayout Grid.Column="0" WidthRequest="15" >
                                                <BoxView Color="#93d6e7"
                                                     WidthRequest="0"
                                                     HorizontalOptions="Center" 
                                                     VerticalOptions="FillAndExpand"/>
                                            </StackLayout>
                                            <Label 
                                               AutomationId="AGatedOut"
                                               Text="{Binding GatedOut}" 
                                               StyleClass="Statusfont2, Statustext2" 
                                               VerticalOptions="Start" 
                                               HorizontalOptions="StartAndExpand" 
                                               Grid.Column="1"></Label>
                                        </StackLayout>
                                    </Grid>
                                </StackLayout>
                            </Frame>
                        </ScrollView>
                    </Frame>
                    <!--Export-->
                    <!--12-01-2022-Sanduru-->
                    <Frame IsVisible ="{Binding IsVisibleBlExport}"
                        BackgroundColor="{DynamicResource Trackingstatusframe}"
                       Grid.Row="2" 
                       CornerRadius="20"  
                       HasShadow="False" 
                       VerticalOptions="EndAndExpand" 
                       Padding="20">
                        <ScrollView>
                            <Frame BackgroundColor="Transparent" 
                               xct:ShadowEffect.Radius="20" 
                               HasShadow="False" 
                               CornerRadius="20" 
                               Padding="5" 
                               Margin="0,10,0,0">
                                <StackLayout Orientation="Vertical" 
                                         VerticalOptions="StartAndExpand" 
                                         Spacing="0">
                                    <Grid RowSpacing="0">
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="Auto"/>
                                            <RowDefinition Height="Auto"/>
                                        </Grid.RowDefinitions>
                                        <StackLayout Orientation="Horizontal" 
                                                     Margin="0,0,0,0"
                                                     Grid.Row="1">
                                            <Grid>
                                                <Grid.ColumnDefinitions>
                                                    <ColumnDefinition Width="20"/>
                                                    <ColumnDefinition Width="Auto"/>
                                                </Grid.ColumnDefinitions>
                                            </Grid>
                                            <Image  
                                               Source="{Binding Imgexpgatedin}" 
                                               WidthRequest="15" 
                                               Grid.Column="0"/>
                                            <Label   
                                               Text="{Binding lblGatedin}" 
                                               StyleClass="Statusfont1, Statustext1" 
                                               VerticalOptions="CenterAndExpand" 
                                               HorizontalOptions="StartAndExpand" 
                                               Grid.Column="1" ></Label>
                                        </StackLayout>
                                        <StackLayout Orientation="Horizontal" 
                                                     Margin="0,0,0,0" 
                                                     Grid.Row="2">
                                            <Grid>
                                                <Grid.ColumnDefinitions>
                                                    <ColumnDefinition Width="20"/>
                                                    <ColumnDefinition Width="Auto"/>
                                                </Grid.ColumnDefinitions>
                                            </Grid>
                                            <StackLayout Grid.Column="0" 
                                                         WidthRequest="15" >
                                                <BoxView Color="#93d6e7" 
                                                     WidthRequest="1" 
                                                     HorizontalOptions="Center" 
                                                     VerticalOptions="FillAndExpand"/>
                                            </StackLayout>
                                            <Label  
                                               Text="{Binding ReceivedGatedIn}" 
                                               StyleClass="Statusfont2, Statustext2" 
                                               VerticalOptions="Start" 
                                               HorizontalOptions="StartAndExpand" 
                                               Grid.Column="1"></Label>
                                        </StackLayout>
                                    </Grid>
                                    <Grid RowSpacing="0">
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="Auto"/>
                                            <RowDefinition Height="Auto"/>
                                        </Grid.RowDefinitions>
                                        <StackLayout Orientation="Horizontal" 
                                                     Margin="0,0,0,0"
                                                     Grid.Row="1">
                                            <Grid>
                                                <Grid.ColumnDefinitions>
                                                    <ColumnDefinition Width="20"/>
                                                    <ColumnDefinition Width="Auto"/>
                                                </Grid.ColumnDefinitions>
                                            </Grid>
                                            <Image 
                                               Source="{Binding Imgexpcustomsinspection}" 
                                               WidthRequest="15" 
                                               Grid.Column="0"/>
                                            <Label x:Name="lblCustomsInspection"  
