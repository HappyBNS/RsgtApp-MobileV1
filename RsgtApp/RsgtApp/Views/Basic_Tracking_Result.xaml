<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:xct="http://xamarin.com/schemas/2020/toolkit"
             x:Class="RsgtApp.Views.Basic_Tracking_Result" 
             FlowDirection="{DynamicResource pagedirection}" 
             BackgroundImageSource="{DynamicResource Primarybgr}" 
             NavigationPage.HasNavigationBar="True" 
             NavigationPage.HasBackButton="True">
    <Shell.BackButtonBehavior>
        <BackButtonBehavior IsEnabled="False" 
                            TextOverride="." />
    </Shell.BackButtonBehavior>
    <ContentPage.ToolbarItems>
        <!--<ToolbarItem Icon="notification_24.png"  Priority="1" Order="Primary"/>-->
        <!-- <ToolbarItem Icon="abouticon.png" Priority="2" Order="Primary"/>
        <ToolbarItem Text="Settings" Priority="1" Order="Secondary"/>-->
    </ContentPage.ToolbarItems>
    <ContentPage.Content>
        <AbsoluteLayout>
            <!--Gokul20220916 Remove AbosulteLayout-->
            <StackLayout  IsEnabled ="{Binding StackTrackingResult}" 
                       Orientation="Vertical" 
                       VerticalOptions="StartAndExpand" 
                       Spacing="0" 
                       Padding="10,0,10,0"  
                       IsVisible="true" 
                       AbsoluteLayout.LayoutBounds="0,0,1,1"
        AbsoluteLayout.LayoutFlags="All">
                <Grid RowSpacing="0" >
                    <Grid.RowDefinitions>
                        <RowDefinition Height="Auto"></RowDefinition>
                        <RowDefinition Height="Auto"></RowDefinition>
                        <RowDefinition Height="*"></RowDefinition>
                        <RowDefinition Height="Auto"></RowDefinition>
                    </Grid.RowDefinitions>
                    <StackLayout Spacing="0" 
                             VerticalOptions="StartAndExpand" 
                             Margin="0,0,0,10" 
                             Padding="0" 
                             Grid.Row="0">
                        <Image  
                           Source="track_icon_blue.png" 
                           VerticalOptions="Start" 
                           HorizontalOptions="Center" 
                           Margin="0,0,0,0"  
                           WidthRequest="170" 
                           HeightRequest="70" ></Image>
                        <Label  
                           Text="{Binding LblTrackingResult}" 
                           Margin="0,0,0,0" 
                           StyleClass="txt24,textsemibold" 
                           HorizontalTextAlignment="Center" 
                           TextColor="{DynamicResource Primary}"  
                           HorizontalOptions="CenterAndExpand"/>
                    </StackLayout>
                    <StackLayout VerticalOptions="StartAndExpand"
                                 Margin="0,0,0,10"
                                 Padding="0" 
                                 Grid.Row="1" >
                        <Grid>
                            <Frame BackgroundColor="{DynamicResource Trackingframe}"
                                   CornerRadius="20"
                                   Margin="0,10,0,0"
                                   Padding="20"
                                   HasShadow="False">
                                <xct:Expander>
                                    <xct:Expander.Header>
                                        <Grid>
                                            <Grid.RowDefinitions>
                                                <RowDefinition Height="*"/>
                                                <RowDefinition Height="*"/>
                                                <RowDefinition Height="*"/>
                                            </Grid.RowDefinitions>
                                            <StackLayout Orientation="Horizontal">
                                                <Label  
                                                   Text="{Binding LblBayanNo}" 
                                                   StyleClass="Frametextcolor,Frametextbold" />
                                                <Label 
                                                   Text="{Binding BayanNo}" 
                                                   StyleClass="Frametextcolor,Frametextregular"/>
                                            </StackLayout>
                                            <Image  
                                                   AutomationId="AimgDownArrowIcon"
                                               Source="{Binding ImgDownArrowIcon}" 
                                               HorizontalOptions="End" 
                                               VerticalOptions="Center" 
                                               HeightRequest="10">
                                                <Image.Triggers>
                                                    <DataTrigger TargetType="Image"
                                                                 Binding="{Binding Source={RelativeSource AncestorType={x:Type xct:Expander}}, Path=IsExpanded}" 
                                                                 Value="True">
                                                        <Setter Property="Source" 
                                                                Value="up_arrow_white.png" />
                                                    </DataTrigger>
                                                </Image.Triggers>
                                            </Image>
                                            <StackLayout Orientation="Horizontal"
                                                         Grid.Row="1">
                                                <Label  Text="{Binding LblBol}" 
                                                        StyleClass="Frametextcolor,Frametextbold" />
                                                <Label Text="{Binding BLNo}" 
                                                       StyleClass="Frametextcolor,Frametextregular"/>
                                            </StackLayout>
                                            <StackLayout Orientation="Horizontal" Grid.Row="2">
                                                <Label Text="{Binding LblContainer}"
                                                       StyleClass="Frametextcolor,Frametextbold" />
                                                <Label Text="{Binding ContainerNo}" 
                                                       StyleClass="Frametextcolor,Frametextregular"/>
                                            </StackLayout>
                                            <!--12-01-2022-Sanduru-->
                                            <StackLayout Orientation="Horizontal" Grid.Row="3">
                                                <Label  Text="{Binding lblCategory}" 
                                                        StyleClass="Frametextcolor,Frametextbold" />
                                                <Label Text="{Binding CategoryImpExp}"
                                                       StyleClass="Frametextcolor,Frametextregular"/>
                                            </StackLayout>
                                        </Grid>
                                    </xct:Expander.Header>
                                    <StackLayout VerticalOptions="StartAndExpand" 
                                                 Margin="0,5,0,10" 
                                                 Padding="0">
                                        <StackLayout Orientation="Horizontal" >
                                            <Label  Text="{Binding LblVesselVisitId}"
                                                    StyleClass="Frametextcolor,Frametextbold" />
                                            <Label  Text="{Binding VesselVisitId}" 
                                                    StyleClass="Frametextcolor,Frametextregular"/>
                                        </StackLayout>
                                        <StackLayout Orientation="Horizontal" >
                                            <Label Text="{Binding LblVesseName}" 
                                                   StyleClass="Frametextcolor,Frametextbold" />
                                            <Label  Text="{Binding VesseName}" 
                                                    StyleClass="Frametextcolor,Frametextregular"/>
                                        </StackLayout>
                                        <StackLayout Orientation="Horizontal">
                                            <Label  Text="{Binding LblVoyage}" 
                                                    StyleClass="Frametextcolor,Frametextbold" />
                                            <Label  Text="{Binding Voyage}" 
                                                    StyleClass="Frametextcolor,Frametextregular"/>
                                        </StackLayout>
                                        <StackLayout Orientation="Horizontal">
                                            <Label  Text="{Binding LblETA}" 
                                                    StyleClass="Frametextcolor,Frametextbold" />
                                            <Label  Text="{Binding ETA}"
                                                    StyleClass="Frametextcolor,Frametextregular"/>
                                        </StackLayout>

                                        <StackLayout Orientation="Horizontal" 
                                                     IsVisible="{Binding IsVisibleEmptyDepot}">
                                            <Label  Text="{Binding LblEmptyDepotFreeDays}"
                                                    StyleClass="Frametextcolor,Frametextbold" />
                                            <Label  Text="{Binding EmptyDepot}"
                                                    StyleClass="Frametextcolor,Frametextregular"/>
                                        </StackLayout>

                                        <StackLayout Orientation="Horizontal"
                                                     IsVisible="{Binding IsVisibleDepotFreeDays}">
                                            <Label  Text="{Binding LblDepotFreeDays}" 
                                                    StyleClass="Frametextcolor,Frametextbold" />
                                            <Label  Text="{Binding DepotFreeDays}" 
                                                    StyleClass="Frametextcolor,Frametextregular"/>
                                        </StackLayout>
                                    </StackLayout>
                                </xct:Expander>
                            </Frame>
                        </Grid>
                    </StackLayout>
                    <!--Import-->
                    <!--12-01-2022-Sanduru-->
                    <Frame IsVisible ="{Binding IsVisibleBlImport}"
                        BackgroundColor="{DynamicResource Trackingstatusframe}" 
                       Grid.Row="2" 
                       CornerRadius="20"  
                       HasShadow="False" 
                       VerticalOptions="EndAndExpand" 
                       Padding="20">
                        <ScrollView>
                            <Frame BackgroundColor="Transparent" 
                                   HasShadow="False" 
                                   xct:ShadowEffect.Radius="20" 
                                   CornerRadius="20" 
                                   Padding="5" 
                                   Margin="0,0,10,0">
                                <StackLayout Orientation="Vertical" 
                                         VerticalOptions="StartAndExpand" 
                                         Spacing="0">
                                    <Grid RowSpacing="0">
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="Auto"/>
                                            <RowDefinition Height="Auto"/>
                                        </Grid.RowDefinitions>
                                        <StackLayout Orientation="Horizontal" 
                                                     Margin="0,0,0,0" 
                                                     Grid.Row="1">
                                            <Grid>
                                                <Grid.ColumnDefinitions>
                                                    <ColumnDefinition Width="20"/>
                                                    <ColumnDefinition Width="Auto"/>
                                                </Grid.ColumnDefinitions>
                                            </Grid>
                                            <Image Source="{Binding ImgStatusIconOnVessel}"
                                                   WidthRequest="15" 
                                                   Grid.Column="0"/>
                                            <Label 
                                               Text="{Binding LblOnVessel}" 
                                               StyleClass="Statusfont1, Statustext1" 
